#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int n;
    double x;
    float y;
    float res = 0;
    
    for(x = 0.1; x <= 1.2; x+=0.1)
    {
        res = 0;
        for(n = 0; n <= 20; n++)
        {
            y = (pow(2*x, n)) / (n + 1);
            res += y;
        }
        cout << "y(" << x << ") = " << res << "\n";
    }

    return 0;
}
