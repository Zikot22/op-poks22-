#include <iostream>
#include <cmath>

using namespace std;

int main()
{
    int n;
    double x;
    float y;
    float res = 0;
    
    for(x = 0.1; x <= 1.2; x+=0.1)
    {
        res = 0;
        for(n = 1; n <= 20; n++)
        {
            y = (((n*n) + 1) / n) * (pow(x/2, n));
            res += y;
        }
        cout << "y(" << x << ") = " << res << "\n";
    }

    return 0;
}
